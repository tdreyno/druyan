{
  "name": "@druyan/druyan",
  "version": "3.0.0-alpha.11",
  "scripts": {
    "lint": "tslint --project .",
    "build": "pika build && yarn compress",
    "compress": "terser pkg/dist-web/index.js -o pkg/dist-web/index.min.js --source-map --mangle --compress passes=2 --toplevel --mangle-props regex=/_$/ && gzip-size pkg/dist-web/index.min.js",
    "test": "jest"
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-ts-standard-pkg",
        {
          "exclude": [
            "__tests__/**/*"
          ]
        }
      ],
      [
        "@pika/plugin-build-node"
      ],
      [
        "@pika/plugin-build-web"
      ]
    ]
  },
  "devDependencies": {
    "@pika/pack": "^0.5.0",
    "@pika/plugin-build-node": "^0.9.0",
    "@pika/plugin-build-web": "^0.8.1",
    "@pika/plugin-ts-standard-pkg": "^0.8.1",
    "@types/lodash.flatten": "^4.4.6",
    "@types/lodash.isplainobject": "^4.0.6",
    "@types/lodash.mapvalues": "^4.6.6",
    "@types/serialize-javascript": "^1.5.0",
    "gzip-size-cli": "^3.0.0",
    "serialize-javascript": "^2.1.0",
    "terser": "^4.1.4",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-immutable": "^6.0.1",
    "tslint-microsoft-contrib": "^6.1.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "@tdreyno/pretty-please": "^1.1.1",
    "lodash.flatten": "^4.4.0",
    "lodash.isplainobject": "^4.0.6",
    "lodash.mapvalues": "^4.6.0"
  }
}
