{
  "name": "@druyan/druyan-react",
  "version": "3.0.0-alpha.11",
  "main": "build/index.js",
  "typings": "build/index.d.ts",
  "scripts": {
    "lint": "tslint --project .",
    "build": "pika build && yarn compress",
    "compress": "terser pkg/dist-web/index.js -o pkg/dist-web/index.min.js --source-map --mangle --compress passes=2 --toplevel --mangle-props regex=/_$/ && gzip-size pkg/dist-web/index.min.js",
    "test": "echo 'No Tests'"
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-ts-standard-pkg",
        {
          "exclude": [
            "__tests__/**/*"
          ]
        }
      ],
      [
        "@pika/plugin-build-node"
      ],
      [
        "@pika/plugin-build-web"
      ]
    ]
  },
  "dependencies": {
    "@druyan/druyan": "^3.0.0-alpha.11",
    "lodash.isfunction": "^3.0.9"
  },
  "peerDependencies": {
    "react": "^16.8.4",
    "react-dom": "^16.8.4"
  },
  "devDependencies": {
    "@pika/pack": "^0.5.0",
    "@pika/plugin-build-node": "^0.8.1",
    "@pika/plugin-build-web": "^0.8.1",
    "@pika/plugin-ts-standard-pkg": "^0.9.0",
    "@types/lodash": "^4.14.126",
    "@types/lodash.isfunction": "^3.0.6",
    "@types/react": "^16.8.8",
    "@types/react-dom": "^16.8.2",
    "gzip-size-cli": "^3.0.0",
    "terser": "^4.1.4",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-immutable": "^6.0.1",
    "tslint-microsoft-contrib": "^6.1.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.7.2"
  }
}
